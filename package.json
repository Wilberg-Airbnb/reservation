{
  "name": "reservation",
  "version": "1.0.0",
  "description": "reservation widget for airbrb",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "file-loader": "^6.0.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "test": "jest -c jest.config.js --coverage",
    "test:watch": "jest -c jest.config.js --coverage --watch --silent --runInBand --detectOpenHandles",
    "test:collect": "jest --collect-coverage",
    "test:update": "jest --update-snapshot",
    "seed": "node db/seed.js",
    "react:dev": "webpack -d --watch",
    "react:pro": "webpack -p --watch",
    "server:dev": "nodemon server/server.js",
    "start": "node server/server.js",
    "containers": "concurrently \"npm run seed\" \"npm run start\""
  },
  "jest": {
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wilberg-Airbnb/reservation.git"
  },
  "keywords": [
    "FEC"
  ],
  "author": "sam johnson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Wilberg-Airbnb/reservation/issues"
  },
  "homepage": "https://github.com/Wilberg-Airbnb/reservation#readme"
}
